{
  "name": "sojourns",
  "private": true,
  "version": "0.1.0",
  "description": "Minimal Next.js app scaffold for Sojourns",
  "type": "module",
  "engines": {
    "node": ">=18.17"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "fmt:check": "prettier -c .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --coverage --pool=threads",
    "test:watch": "vitest",
    "fmt": "prettier -w .",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.886.0",
    "@aws-sdk/s3-request-presigner": "^3.886.0",
    "@clerk/nextjs": "^5.0.0",
    "@neondatabase/serverless": "^1.0.1",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.5",
    "nanoid": "^5.1.5",
    "next": "^15.0.0",
    "openai": "^5.20.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "svix": "^1.76.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.66",
    "@vitest/coverage-v8": "^3.2.4",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.8.0",
    "eslint-config-next": "^15.0.0",
    "prettier": "^3.3.3",
    "tsx": "^4.20.5",
    "typescript": "^5.4.5",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "esbuild": "^0.25.9",
    "@esbuild-kit/core-utils": {
      "esbuild": "^0.25.9"
    }
  }
}
